<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zes.squad.gmh.web.mapper.MemberLevelMapper">
	<resultMap id="BaseResultMap" type="com.zes.squad.gmh.web.entity.po.MemberLevelPo">
		<id column="id" jdbcType="BIGINT" property="id" />
		<result column="store_id" jdbcType="BIGINT" property="storeId" />
		<result column="name" jdbcType="VARCHAR" property="bane" />
		<result column="created_time" jdbcType="TIMESTAMP" property="createdTime" />
		<result column="modified_time" jdbcType="TIMESTAMP" property="modifiedTime" />
	</resultMap>
	<sql id="Base_Column_List">
		`id`,`store_id`,`name`,
		`created_time`,
		`modified_time`
	</sql>
	<select id="getAll" parameterType="Long" resultMap="BaseResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM member_level WHERE store_id=#{storeId, jdbcType=BIGINT}
	</select>
	<insert id="insert" parameterType="com.zes.squad.gmh.web.entity.po.MemberLevelPo">
		INSERT INTO
		member_level(store_id,name)
		VALUES
		(#{storeId, jdbcType=BIGINT},#{name,
		jdbcType=VARCHAR})
	</insert>
	<update id="update" parameterType="com.zes.squad.gmh.web.entity.po.MemberLevelPo">
		UPDATE member_level SET
		name=#{name, jdbcType=VARCHAR}
		WHERE id=#{id, jdbcType=BIGINT}
	</update>
	<delete id="delById" parameterType="long">
		DELETE FROM member_level
		WHERE id=#{id, jdbcType=BIGINT}
	</delete>
	<delete id="batchDelete" parameterType="list">
		DELETE FROM
		member_level
		WHERE id IN
		<foreach collection="list" item="id" separator="," open="("
			close=")">
			#{id, jdbcType=BIGINT}
		</foreach>
	</delete>
</mapper>